<h1>Homepage</h1> <h2 id="welcomeMessage">Welcome <%= name %> </h2>
<form action="/logout" method="POST">
    
    <p>Not <%= name %>?</p>
<input type="submit" value="Sign out" id="logOutButton"></form>
<hr>
<h3>Chitters:</h3>
<form action="/add" method=GET>
<input style="position:relative; margin-top: -39px ;left: 100pt;" type="submit" value="Add Chitter" id="addChitter"></form>
<hr>



<div id='chitter'>
     <% chitter.map((peep, index) => {%>
    <%= `${peep.User.name} - ${peep.User.username}:` %>
    <p id="chitter-<%=index%>"><%= peep.chitter %></p>
    <form action="/home/<%=peep.id%>?_method=DELETE" method="POST">
    <input type="submit" value="Delete Chitter" id="deleteChitterButton"></form>
    <%= peep.createdAt %> <hr>
    <% })%>

    </div>